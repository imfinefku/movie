/**app.wxss**/
@import 'style/weui.wxss';

.container {
  height: 100%;
  background: #f7f7f7;
  display: flex;
  flex-direction: column;
  box-sizing: border-box;
}

.block {
  background: #fff;
  margin-bottom: 12rpx;
}

.block .title {
  font-size: 32rpx;
  font-weight: bold;
  padding: 20rpx;
}

.block .content{
  font-size: 30rpx;
  color: #666;
  padding: 0 16rpx 16rpx 16rpx;
}